    "version": "0.2.0",
    "configurations": [
        {
            "name": "Upload Files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/csutil",
            "args": ["push", "kronos-testdata-nicky", "./dat/push_folder/*.txt "],
            "console": "integratedTerminal"
        },
        {
            "name": "Download Files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/csutil",
            "args": ["pull", "kronos-testdata-nicky", "./dat/pull_folder", "* "],
            "console": "integratedTerminal"
        },
        {
            "name": "List Bucket",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/csutil",
            "args": ["list-remote", "kronos-training-input-stage"],
            "console": "integratedTerminal"
        }
